<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/studentFragment">
    <fragment
        android:id="@+id/studentFragment"
        android:name="com.example.myapplication.ui.student.StudentFragment"
        android:label="fragment_student"
        tools:layout="@layout/fragment_student" >
        <action
            android:id="@+id/action_studentFragment_to_studentAddDialog"
            app:destination="@id/studentAddDialog" />
    </fragment>
    <dialog
        android:id="@+id/studentAddDialog"
        android:name="com.example.myapplication.ui.student.dialog.AddStudentDialog"
        android:label="dialog_student"
        tools:layout="@layout/dialog_add">
        <argument
            android:name="student"
            app:argType="com.example.myapplication.model.dataclass.EntityStudent"
            app:nullable="true"
            />
    </dialog>
</navigation>